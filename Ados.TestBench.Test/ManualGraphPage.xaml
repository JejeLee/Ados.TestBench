                    </d3:ChartPlotter.VerticalAxis>
                    <d3:VerticalAxis Placement="Right" ShowMayorLabels="True" ShowMinorTicks="True" >
                        <d3:VerticalAxisTitle Content="{Binding Path=VerticalTitle}"/>
                    </d3:VerticalAxis>
